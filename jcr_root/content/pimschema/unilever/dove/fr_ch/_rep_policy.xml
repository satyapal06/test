<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:crx="http://www.day.com/crx/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dove_fr_ch_uat_superauthor"
        rep:privileges="{Name}[jcr:versionManagement,jcr:modifyProperties,jcr:read,crx:replicate,jcr:lockManagement]"/>
    <allow0
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dove_fr_ch_uat_superauthor"
        rep:privileges="{Name}[jcr:removeChildNodes,jcr:removeNode,jcr:addChildNodes,jcr:nodeTypeManagement]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/jcr:content*"/>
    </allow0>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dove_fr_ch_uat_author"
        rep:privileges="{Name}[jcr:read]"/>
</jcr:root>
